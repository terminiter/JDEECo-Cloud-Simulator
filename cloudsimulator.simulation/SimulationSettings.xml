<SimulationSettings>
  <SimulationRun>
    <Profiles>
      <Profile Id="default">
        <SimulationEnd Type="RealTime" Time="600000"/>
      </Profile>
    </Profiles>
  </SimulationRun>
  <Asserts>
    <Default Actions="Log"/>
    <Profiles>
      <Profile Id="assertProfile1">
        <Default reference="../../../Default"/>
        <Assert Id="Severe" Actions="ExitRun"/>
      </Profile>
    </Profiles>
  </Asserts>
  <Statistics>
    <Default Save="Min,Max"/>
    <Profiles>
      <Profile Id="profile1">
        <Default Save="Min"/>
        <Statistic Type="Plain" Id="Hit" Save="Count"/>
        <Statistic Type="Plain" Id="HulaHula" Save="Count"/>
      </Profile>
      <Profile Id="profile2">
        <Default Save="Min,Max,Avg,Vector"/>
        <Import ProfileId="profile1"/>
        <Import ProfileId="profile1"/>
        <Statistic Type="Plain" Id="Test1" Save="Count"/>
        <Statistic Type="Regex" Id="Test2" Save="Vector"/>
      </Profile>
    </Profiles>
  </Statistics>
</SimulationSettings>